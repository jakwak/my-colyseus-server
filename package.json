{
  "private": true,
  "name": "my-app",
  "version": "1.0.0",
  "description": "npm init template for bootstrapping an empty Colyseus project",
  "main": "build/index.js",
  "engines": {
    "node": ">= 20.9.0"
  },
  "scripts": {
    "start": "tsx watch src/index.ts",
    "start:prod": "node --expose-gc build/index.js",
    "start:dev": "node --expose-gc -r tsx/cjs src/index.ts",
    "start:pm2": "tsx src/index.ts",
    "loadtest": "tsx loadtest/example.ts --room my_room --numClients 2",
    "build": "npm run clean && tsc",
    "clean": "rimraf build",
    "test": "mocha -r tsx test/**_test.ts --exit --timeout 15000",
    "monitor": "node remote-monitor.js",
    "monitor:restart": "node remote-monitor.js --restart",
    "monitor:status": "node remote-monitor.js --status",
    "remote:status": "./remote-control.sh status",
    "remote:start": "./remote-control.sh start",
    "remote:restart": "./remote-control.sh restart",
    "remote:stop": "./remote-control.sh stop",
    "remote:logs": "./remote-control.sh logs",
    "remote:monitor": "./remote-control.sh monitor",
    "remote:auto": "./remote-control.sh auto"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/colyseus/create-colyseus/issues"
  },
  "homepage": "https://github.com/colyseus/create-colyseus#readme",
  "devDependencies": {
    "@colyseus/loadtest": "^0.16.0",
    "@colyseus/testing": "^0.16.0",
    "@types/express": "^4.17.1",
    "@types/matter-js": "^0.19.8",
    "@types/mocha": "^10.0.1",
    "mocha": "^10.2.0",
    "rimraf": "^5.0.0",
    "tsx": "^4.10.2",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@colyseus/monitor": "^0.16.0",
    "@colyseus/playground": "^0.16.0",
    "@colyseus/tools": "^0.16.0",
    "colyseus": "^0.16.0",
    "express": "^4.18.2",
    "matter-js": "^0.20.0"
  }
}
